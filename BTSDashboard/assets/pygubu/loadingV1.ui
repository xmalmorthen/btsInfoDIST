<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object id="fraLoading" class="ttk.Frame">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
    </layout>
    <child>
      <object id="fraComponents" class="ttk.Frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object id="lblGif" class="ttk.Label">
            <property name="anchor">center</property>
            <property name="cursor">arrow</property>            
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="rowspan">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="lblTxt" class="ttk.Label">
            <property name="font">{TkCaptionFont } 36 {bold}</property>
            <property name="text" translatable="yes">PROCESANDO</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">s</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="lblTxt2" class="ttk.Label">
            <property name="font">{TkCaptionFont } 12 {bold}</property>
            <property name="text" translatable="yes">FAVOR DE ESPERAR...</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">n</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="LblRightSpan" class="ttk.Label">
            <property name="text" translatable="yes">              </property>
            <layout>
              <property name="column">2</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="rowspan">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
